<?xml version="1.0" encoding="utf-8"?>
<TableLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:id="@+id/minimum_maximum_dialog_layout_roots"
	android:stretchColumns="0">
	<TableRow android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/header_row">
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/component_column"
			android:text="Component"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/male_column"
			android:text="Males" android:paddingLeft="20dip"
			android:paddingRight="20dip"></TextView>
		<TextView android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:id="@+id/female_column"
			android:text="Females" android:paddingLeft="20dip"
			android:paddingRight="20dip"></TextView>
	</TableRow>

	<TableRow android:padding="10dip"></TableRow>

	<TableRow android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/waist_row">
		<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Waist"></TextView>
	<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/waist_male"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/waist_female"></TextView>
</TableRow>
	<TableRow android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/pushup_row">
		<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Pushups"></TextView>
	<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/pushup_male"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/pushup_female"></TextView>
</TableRow>
	<TableRow android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:id="@+id/situp_row">
		<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Situps"></TextView>
	<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/situp_male"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/situp_female"></TextView>
</TableRow>

<TableRow android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/run_row">
		<TextView android:id="@+id/TextView01" android:layout_width="wrap_content"
			android:layout_height="wrap_content" android:text="Run"></TextView><TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/run_male"></TextView>
<TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/run_female"></TextView>
</TableRow>





</TableLayout>
